<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.htlc.cyjk"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.htlc.cyjk.app.App"
        android:theme="@style/AppTheme">
        <activity android:name="com.htlc.cyjk.app.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".app.activity.PersonActivity"/>
        <activity android:name=".app.activity.PayActivity"/>
        <activity android:name=".app.activity.SettingActivity"/>
        <activity android:name=".app.activity.MessageCenterActivity"/>
        <activity android:name=".app.activity.RecommendationActivity"/>
        <activity android:name=".app.activity.WebActivity"/>

        <meta-data android:name="design_width" android:value="750">
        </meta-data>
        <meta-data android:name="design_height" android:value="1334">
        </meta-data>

    </application>

</manifest>
